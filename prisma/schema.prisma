generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model items {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  name       String   @unique
  price      Float
  image      String   @unique
  categories String[]
  bestSeller Boolean?
}
